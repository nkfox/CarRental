<?xml version="1.0" encoding="UTF-8"?>
<Context antiJARLocking="true" path="/CarRental">
    <Resource name="jdbc/CarRental"
              auth="Container"
              type="javax.sql.DataSource"
              maxActive="1000"
              maxIdle="30"
